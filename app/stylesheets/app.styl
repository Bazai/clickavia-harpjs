.g-button-new
  margin: 27px auto 30px
  width: 154px

.g-button-new.disabled, .g-button-new.busy
  border: 1px solid #CACACA
  color: #FFFFFF
  cursor: default
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.4)

.g-button-new.disabled
  background: none repeat scroll 0 0 lightgrey

.g-slider
  min-height: 24px
  padding: 0 !important
  position: relative
  .slide-display
    height: 100%
    margin: 0 40px
    overflow: hidden
    position: relative
    .slide-line
      position: absolute
      width: 10000px
      .item
        display: block
        float: left
    .dates-checker
      top: 20px
  .nav
    background: none repeat scroll 0 0 #76B718
    border-radius: 18px 18px 18px 18px
    box-shadow: 0 -1px #232323
    height: 36px
    opacity: 0.5
    top: 9px
    width: 36px

    display: block
    height: 100%
    position: absolute
    top: 0
    width: 40px
    span.inner
      get_main_sprite()
      cursor: default
      display: block
      height: 100%
      width: 100%
  .nav.enabled
    opacity: 1 !important

.b-progress-bar
  display: none
  left: 50%
  margin-left: -100px
  position: absolute
  width: 200px
  z-index: 100
  p.progress-bar-title
    color: #FFFFFF
    margin-bottom: 0
    text-align: center
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25)
    transition-duration: 0.5s
    transition-property: text-shadow
  div.progress-bar
    border-radius: 2px 2px 2px 2px
    box-shadow: 0 1px #212121 inset, 0 -1px #949494 inset
    height: 4px
    min-height: 4px
    width: 100%
    span
      background-color: #878787
      border-radius: 1px 0 0 1px
      box-shadow: -3px 0 1px white inset
      display: block
      height: 2px
      position: relative
      top: 1px
      width: 0

