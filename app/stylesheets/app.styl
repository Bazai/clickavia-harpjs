.l-page__specials
  background-color: #343434
  color: #C6C6C6
  font-family: "Myriad Pro", Helvetica, Arial, Tahoma, sans-serif
  min-height: 502px
  overflow: hidden
  a
    color: #C6C6C6
  .l-page__center

    min-height: 502px
    overflow: hidden
    padding: 0 18px
    width: 929px
    border-left: 1px solid #5c5c5c
    border-right: 1px solid #5c5c5c
