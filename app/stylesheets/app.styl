@import '_nib'
@import 'global/_reset.styl'

get_common_sprite()
  background: url('/app/images/common_sprite.png') no-repeat
get_main_sprite()
  background: url('/app/images/main_sprite.png') no-repeat

h1, h2, h3
  font-family: "PFDinTextCondProThin", Tahoma, Arial, Verdana, sans-serif
  font-style: normal
  font-weight: normal

.brd_left
  position: absolute
  border-left: 1px solid #5C5C5C
  z-index: 10000

.brd_right
  position: absolute
  border-right: 1px solid #212121
  z-index: 10000

.left_footer_border
  border-left: 1px solid #5c5c5c

.right_footer_border
  border-right: 1px solid #5c5c5c
  height: 428px !important
  margin-top: 0 !important
  margin: 0 !important
  width: 303px !important

.g-button-new
  background-color: #4A4A4A
  border: 1px solid #3D3D3D
  border-radius: 3px 3px 3px 3px
  color: #FFFFFF
  cursor: pointer
  font-size: 14px
  font-weight: bold
  height: 41px
  line-height: 41px
  text-align: center
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.8)
  transition-duration: 0.25s
  transition-property: color, background-color, border-color

.l-page__center:before,
.l-page__center:after
  content: ""
  display: table
  clear: both

.l-page__center
  margin: 0 auto
  padding: 0 15px
  width: 928px
  .inner
    margin-left: -16px
    padding: 0 17px
    width: 924px

button, input, select, textarea
  color: #333333
  font-family: "Myriad Pro", Helvetica, Arial, Tahoma, sans-serif
  font-size: 14px
  outline: medium none

body
  cursor: default
  line-height: 17px
  background-color: #F2F2F2
  height: 256px
  font-family: "Myriad Pro", Helvetica, Arial, Tahoma, sans-serif
  font-size: 14px
  min-height: 100%
  color: #333333
  position: relative
  min-width: 958px
  a
    color: #76B718
    text-decoration: none
  .l-page
    header
      background-color: #343434
      z-index: 1010
      height: 44px
      .container
        .brd_left
          height: 43px
          left: 1px
          top: 1px
        .brd_right
          height: 44px
          top: 0px
          right: 1px
        height: 33px
        margin-left: -21px
        overflow: hidden
        padding-left: 20px
        padding-right: 20px
        padding-top: 11px
        transition-duration: 200ms
        transition-property: height
        width: 926px
        position: relative
        border-left: 1px solid #212121
        border-right: 1px solid #5c5c5c
        .logo
          display: inline-block
          margin-right: 15px
          a.inline-logo
            get_common_sprite()
            background-position: 0 -210px
            background-repeat: no-repeat
            display: inline-block
            height: 24px
            width: 114px
            &:hover
              background-position: 0px -235px
        .social-links
          display: inline-block
          a
            get_common_sprite()
            background-repeat: no-repeat
            display: inline-block
            height: 25px
            width: 24px
            margin-left: 3px
          a.facebook
            background-position: 0 -265px
            &:hover
              background-position: 0px -290px
          a.twitter
            background-position: -24px -265px
            &:hover
              background-position: -24px -290px
          a.vkontakte
            background-position: -48px -265px
            &:hover
              background-position: -48px -290px
        .user-control
          position: absolute
          right: 20px
          top: 5px
          #user_auth
            margin-top: 8px
            & > *
              display: inline-block
              font-family: "Myriad Pro", Helvetica, Arial, Tahoma, sans-serif
              font-size: 15px
              line-height: 24px
              text-shadow: 0 -1px 0 #0E0E0E
              white-space: nowrap
            & > a
              color: #E6E6E6
              cursor: pointer
              &:hover
                color: #ffffff
                text-decoration: none
            span
              color: #878787
    .l-page__header-bottom
      background: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAkAAAAMCAYAAACwXJejAAAAAXNSR0IArs4c6QAAAAZiS0dEAAAAAAAA%2BUO7fwAAAAlwSFlzAAALEwAACxMBAJqcGAAAAAd0SU1FB9wJGQk4GmwFcp4AAAA8SURBVBjTY%2FTy8vrPQAAwCgsLE1TExEAEoJ4iRgYGhlRCilgYGBieEaPoNTGK3hGj6CMxir4R4ztmQooAOBsIQvhc190AAAAASUVORK5CYII%3D") repeat-x scroll 0 0 transparent
      height: 12px
      margin-bottom: -7px
      z-index: 1010
      .grid-bottom
        background: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAA8gAAAAGCAYAAAAfWLdbAAAAAXNSR0IArs4c6QAAAAZiS0dEAP8A%2FwD%2FoL2nkwAAAAlwSFlzAAALEwAACxMBAJqcGAAAAAd0SU1FB9wKEAgVBI1KSn8AAADYSURBVHja7dCxbUIxFIXhQwq%2FAuwJXCExAhKCxg3No0NI0RvH4zBIai%2BRCSy5oDppEylZ4OX%2FyntP9W8kvffeP1NKk6QPSS8BAAAAALB%2Bk6RL7%2F2VUspvkp4xxs2yLFtJR%2FoAAAAAAP6J47Is2xjjRtJTkmQ7jTHmnPNd0oFGAAAAAICVO%2BSc72OM2Xb68bG9b63NIYSHpEQrAAAAAMBKpRDCo7U2297%2FurB9qrXeJF3pBQAAAABYqWut9Wb79OfC9mT7XErZ0QsAAAAAsEallJ3ts%2B3p%2B%2F0LpFJBw2ZUCZ4AAAAASUVORK5CYII%3D") repeat-y scroll 0 0 transparent
        height: 6px
        margin-left: -20px
        width: 968px
    .l-page__header-top-offset
      .inner
        height: 34px
        border-left: 1px solid #e4e4e4
        border-right: 1px solid #e4e4e4
    .l-page__top-line
      background-color: #F2F2F2
      font-family: "Myriad Pro", Helvetica, Arial, Tahoma, sans-serif
      font-size: 14px
      overflow: hidden
      .l-page__top-line__shadow
        .l-page__center
          border-left: 1px solid #e4e4e4
          border-right: 1px solid #e4e4e4
          width: 928px
          .left-bar
            float: left
            font-family: "Myriad Pro", Helvetica, Arial, Tahoma, sans-serif
            width: 272px
            .logo
              display: inline-block
              margin-top: 54px
              get_main_sprite()
              background-position: 0 -480px
              width: 135px
              height: 120px
            p.about
              font-size: 14px
              line-height: 20px
              margin: 40px 0 0
              text-shadow: 0 1px 0 white
            .short-menu
              display: inline-block
              font-size: 18px
              line-height: 38px
              margin-top: 36px
              text-align: left
            .short-menu__item:hover
              color: #568511
            .short-menu__item
              get_main_sprite()
              background-position: 181px -1250px
              color: #67A015
              display: inline-block
              font-family: Georgia, "Nimbus Roman No9 L", serif
              font-size: 16px
              font-style: italic
              height: 40px
              margin-top: 14px
              padding-right: 48px
              text-decoration: underline
              text-shadow: 0 1px 0 white
              width: 173px
              &.how
                &:hover
                  background-position: 181px -1370px
              &.fin
                background-position: 181px -1330px
                cursor: pointer
                &:hover
                  background-position: 181px -1450px
          .right-bar
            margin-left: 272px
            padding-left: 48px
            .pay-ways:after
              content: ""
              height: 0
              width: 99%
              display: inline-block
              text-align: left
              font-size: 2px
              line-height: 0
              text-align: justify
              width: 592px
            .pay-ways
              .item
                background: url('/app/images/main_sprite.png') no-repeat
                display: block
                float: left
                height: 39px
                margin-left: 8px
                vertical-align: middle
              .item.caption
                background: none repeat scroll 0 0 transparent !important
                font-size: 14px
                line-height: 54px
              .item.evroset
                background-position: 0 -893px
                width: 120px
              .item.svyaznoy
                background-position: -120px -893px
                width: 110px
              .item.mastercard
                background-position: -230px -893px
                width: 70px
              .item.visa
                background-position: -300px -893px
                width: 80px

.b-main-picker
  min-height: 425px
  width: 608px
  z-index: 550
  div
    position: relative
  .month
    width: 304px
    thead
      text-align: left
      th
        padding-bottom: 14px
        padding-left: 12px
    td
      cursor: default
      padding-bottom: 6px
      transition-duration: 0.5s
      transition-property: opacity

      .wrapper
        color: #B3DB79
        cursor: default
        display: block
        padding-left: 12px
        position: relative
        text-decoration: none !important
        text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.3)
        transition-duration: 0.5s
        transition-property: color, text-shadow
    .point-wrapper .point
      bottom: -4px
      display: none
      height: 25px
      left: 6px
      line-height: 25px
      padding-right: 1px
      position: absolute
      text-align: center
      width: 25px
    td.right
      display: table-cell
      float: none
      .wrapper
        border-bottom-right-radius: 8px
        border-top-right-radius: 8px

    td.left
      display: table-cell
      float: none
      .wrapper
        border-bottom-left-radius: 8px
        border-top-left-radius: 8px

    table
      table-layout: fixed
      border-collapse: collapse
      width: 100%
    .caption
      background: none repeat scroll 0 0 #333333
      font-size: 18px
      line-height: 54px
      text-align: center
      text-shadow: 0 -1px #232323
    .table-wrapper
      padding: 20px
      get_main_sprite()
      background-position: 0 -601px
  .points-hint
    color: #FFFFFF
    line-height: 18px
    text-align: center
  .shadow
