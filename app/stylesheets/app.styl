  .points-0
    .points-hint
      color: #333333
      text-shadow: none
    .container .fields-line > .small-log
      background-position: center 0
  .container
    background: none repeat scroll 0 0 #76B718
    border-radius: 10px 10px 10px 10px
    color: #FFFFFF
    margin-top: 16px
    text-shadow: 0 -1px #507C10
    z-index: 1050
    .clear-dates
      bottom: 10px
      cursor: pointer
      display: none
      font-size: 13px
      position: absolute
      right: 25px
      span
        cursor: pointer
        text-decoration: underline
        vertical-align: middle
      .button:after
        content: ""
        display: inline-block
        font-family: FontAwesome
        font-size: 11px
        font-style: normal
        font-weight: normal
        padding: 0 2px 0 1px
        text-decoration: none
    .progress-bar-title
      white-space: nowrap
    .month-slider.g-slider
      height: 252px
      line-height: 17px
      margin-top: 18px
      .slide-display
        margin: 0
      .left-nav-placeholder, .right-nav-placeholder
        background: none repeat scroll 0 0 #333333
        height: 54px
        position: absolute
        top: 0
        width: 65px
      .left-nav-placeholder
        left: 0
      .right-nav-placeholder
        right: 0
      .nav.enabled
        span.inner
          cursor: pointer
      .nav.enabled:hover
        background-color: #80C20A
      .nav
        background: none repeat scroll 0 0 #76B718
        border-radius: 18px 18px 18px 18px
        box-shadow: 0 -1px #232323
        height: 36px
        opacity: 0.5
        top: 9px
        width: 36px
      .nav.next
        right: 16px
        span.inner
          background-position: 16px -852px
      .nav.prev
        left: 16px
        span.inner
          background-position: 16px -816px
    .fields-line
      height: 72px
      position: relative
      .small-logo
        height: 50px
        margin: 0 auto
